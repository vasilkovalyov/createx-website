{
  "kind": "collectionType",
  "collectionName": "pages",
  "info": {
    "singularName": "page",
    "pluralName": "pages",
    "displayName": "Page",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Slug": {
      "type": "uid",
      "targetField": "Name"
    },
    "Seo": {
      "type": "component",
      "repeatable": false,
      "component": "seo.seo-information",
      "required": true
    },
    "Name": {
      "type": "string",
      "required": true
    },
    "Body": {
      "type": "dynamiczone",
      "components": [
        "block-benefits.block-benefits",
        "block-benefits.items",
        "block-hero.block-hero",
        "block-intro-carousel.block-intro-carousel",
        "block-our-clients.block-our-clients",
        "block-partners.block-partners"
      ]
    },
    "ShowFormDetails": {
      "type": "boolean"
    },
    "ContentType": {
      "type": "string"
    },
    "pages": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::page.page"
    },
    "Breadcrumbs": {
      "type": "component",
      "repeatable": false,
      "component": "breadcrumbs.breadcrumbs"
    }
  }
