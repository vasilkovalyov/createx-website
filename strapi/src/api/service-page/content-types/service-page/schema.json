{
  "kind": "collectionType",
  "collectionName": "service_pages",
  "info": {
    "singularName": "service-page",
    "pluralName": "service-pages",
    "displayName": "Service Page",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string",
      "required": true
    },
    "Slug": {
      "type": "uid",
      "targetField": "Title"
    },
    "Seo": {
      "type": "component",
      "repeatable": false,
      "component": "seo.seo-information",
      "required": false
    },
    "pages": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::page.page"
    },
    "service": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::service.service",
      "inversedBy": "service_page"
    },
    "ContentType": {
      "type": "string",
      "required": true,
      "unique": false,
      "default": "servicePage"
    },
    "ShowFormDetails": {
      "type": "boolean",
      "default": true
    },
    "Body": {
      "type": "dynamiczone",
      "components": [
        "block-hero.block-hero",
        "block-price.block-price"
      ]
    },
    "BreadcrumbsInner": {
      "type": "component",
      "repeatable": false,
      "component": "breadcrumbs-inner.breadcrumbs-inner"
    }
  }
}
